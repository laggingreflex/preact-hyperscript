{
  "name": "preact-hyperscript",
  "version": "0.3.1",
  "description": "Hyperscript-like syntax for creating Preact elements",
  "main": "lib/index.js",
  "scripts": {
    "test:node": "tape test/*.js | tap-spec",
    "test:lint": "snazzy",
    "test": "npm-run-all -p test:*",
    "prepublish": "mkdirp lib && buble -i src -o lib",
    "release": "xyz"
  },
  "keywords": [
    "hyperscript",
    "preact",
    "element"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/queckezz/preact-hyperscript"
  },
  "author": "queckezz <fabian.eichenberger@gmail.com>",
  "dependencies": {
    "hyperscript-helpers": "^3.0.0",
    "parse-hyperscript": "^0.2.0"
  },
  "peerDependencies": {
    "preact": "*"
  },
  "devDependencies": {
    "buble": "^0.12.5",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^2.3.0",
    "preact": "^5.3.1",
    "preact-render-to-string": "^2.6.1",
    "snazzy": "^4.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "xyz": "^0.5.0"
  },
  "license": "MIT"
}
